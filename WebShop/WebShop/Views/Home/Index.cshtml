@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model List<string>
@{
    ViewData["Title"] = "Home Page";
}
<link rel="stylesheet" href="~/css/Home/index.css" />
<h2 class="title"><b>Zdravlje je naše najveće bogatstvo!</b></h2>
    <div>
    @foreach (var item in Model)
    {
        <img class="slides" src="~/Content/slide_show/@item" alt="Naš OPG" width="384" height="288" align="right" longdesc="kuca.JPG">
    }
</div>
@Html.Partial("~/wwwroot/Html/Index/index.html")
@section Scripts {
    <script src="~/js/Home/index.js"></script>
}
